= form_for :user, url: profile_path, :method => :put do |f|
  = f.label :name
  = f.text_field :name
  = f.label :first_name
  = f.text_field :first_name
  = f.label :last_name
  = f.text_field :last_name
  = f.label :age
  = f.text_field :age
  = f.label :address
  = f.text_field :address
  = f.label :city
  = f.text_field :city
  = f.label :state
  = f.text_field :state
  = f.label :country
  = f.text_field :country
  = f.label :zip_code
  = f.text_field :zip_code
  = f.submit
